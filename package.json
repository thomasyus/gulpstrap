{
  "name": "Gulpstrap",
  "version": "2.0.0",
  "repository": "https://github.com/carlosl/gulpstrap.git",
  "description": "Base gulp configuration with bootrap, jquery and nunjucks templating engine.",
  "homepage": "https://github.com/carlosl/gulpstrap",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "gulp watch",
    "test": "gulp build --prod; exit 0;",
    "postversion": "gulp publish --prod"
  },
  "config": {
    "build_path": "./dist/",
    "src_path": "./src/",
    "enable_browser_sync": true
  },
  "devDependencies": {
    "browser-sync": "^2.11.1",
    "fs-extra": "^0.26.7",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-browserify": "^0.5.1",
    "gulp-clean-css": "^2.0.3",
    "gulp-concat": "^2.6.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-less": "^3.0.5",
    "gulp-newer": "^1.1.0",
    "gulp-nunjucks-render": "^2.0.0",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "yargs": "^4.3.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.2",
    "font-awesome": "^4.5.0",
    "jquery": "^2.2.2",
    "tether": "^1.2.0"
  }
}
